{
  "name": "@thatopen-platform/fragments-beta",
  "description": "Simple geometric system built on top of Three.js to display 3D BIM data efficiently.",
  "version": "3.0.31",
  "author": "That Open Company",
  "contributors": [
    "Antonio Gonzalez Viegas (https://github.com/agviegas)",
    "Juan Hoyos (https://github.com/HoyosJuan)"
  ],
  "license": "MIT",
  "homepage": "https://docs.thatopen.com/",
  "bugs": {
    "url": "https://github.com/ThatOpen/engine_fragment/issues"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ThatOpen/engine_fragment.git",
    "directory": "packages/fragments"
  },
  "scripts": {
    "dev": "vite --host",
    "test": "jest",
    "build": "tsc --p ./tsconfig-build.json && vite build && vite build --config vite.config-worker.ts",
    "prepublishOnly": "yarn build && node ./utils/types-remover.mjs",
    "publish-repo": "npm publish --access restricted",
    "fb": "yarn delete-schema && yarn create-schema",
    "create-schema": "cd ./flatbuffers && flatc.exe --gen-mutable -o ../src/Schema --ts index.fbs",
    "delete-schema": "node ./flatbuffers/delete-schema.mjs",
    "test-node": "yarn tsx ./src/Importers/IfcImporter/node-example.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "flatbuffers": "^24.3.25",
    "lru-cache": "^11.0.2",
    "rxjs": "^7.8.1",
    "uint1array": "^1.2.0"
  },
  "devDependencies": {
    "@thatopen/components": "^2.4.3",
    "@thatopen/components-front": "^2.4.2",
    "@thatopen/fragments": "^2.4.0",
    "@types/jest": "27.0.0",
    "@types/node": "20.11.30",
    "@types/three": "0.160.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "glob": "latest",
    "jest": "^27.0.4",
    "prettier": "3.2.5",
    "stats.js": "^0.17.0",
    "three": "0.170.0",
    "ts-node": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "5.4.2",
    "web-ifc": "^0.0.66"
  },
  "peerDependencies": {
    "three": "^0.170.0",
    "web-ifc": "^0.0.66"
  }
}
